<?xml version="1.0" encoding="UTF-8"?>
<web-app 
  xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0">

	<display-name>badistick</display-name>

	<servlet>
		<servlet-name>AccueilServlet</servlet-name>
		<servlet-class>scaunois.badistick.servlet.AccueilServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AccueilServlet</servlet-name>
		<url-pattern>/accueil</url-pattern>
		<url-pattern>/index</url-pattern>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ConsultationJoueurServlet</servlet-name>
		<servlet-class>scaunois.badistick.servlet.ConsultationJoueurServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ConsultationJoueurServlet</servlet-name>
		<url-pattern>/consultation_joueur</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>RechercheClubServlet</servlet-name>
		<servlet-class>scaunois.badistick.servlet.RechercheClubServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RechercheClubServlet</servlet-name>
		<url-pattern>/recherche_club</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>RechercheJoueurServlet</servlet-name>
		<servlet-class>scaunois.badistick.servlet.RechercheJoueurServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RechercheJoueurServlet</servlet-name>
		<url-pattern>/recherche_joueur</url-pattern>
	</servlet-mapping>
	
	

</web-app>