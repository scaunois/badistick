<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>badistick</display-name>
	
	<servlet>
		<servlet-name>AccueilServlet</servlet-name>
		<servlet-class>scaunois.badistick.servlet.AccueilServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AccueilServlet</servlet-name>
		<url-pattern>/accueil</url-pattern>
		<url-pattern>/index</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>RechercheJoueurServlet</servlet-name>
		<servlet-class>scaunois.badistick.servlet.RechercheJoueurServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>RechercheJoueurServlet</servlet-name>
		<url-pattern>/recherche_joueur</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>RechercheClubServlet</servlet-name>
		<servlet-class>scaunois.badistick.servlet.RechercheClubServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>RechercheClubServlet</servlet-name>
		<url-pattern>/recherche_club</url-pattern>
	</servlet-mapping>

</web-app>